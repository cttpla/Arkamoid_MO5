9500 'HUD
9510 LOCATE 0,0,0:COLOR 6:PRINT "P1":BOXF(64,0)-(144,7),-1:LOCATE 8,0,0:FOR I=1 TO PLRLVS(1)-1:PRINT GR$(1);:NEXT:LOCATE 17,0,0:PRINT"LV"
9520 LOCATE 23,0,0:COLOR 12:PRINT "P2":BOXF(248,0)-(319,7),-1:LOCATE 31,0,0:FOR I=1 TO PLRLVS(2)-1:PRINT GR$(1);:NEXT
9530 LOCATE 2,0,0:COLOR 7:PRINT RIGHT$(STR$(PLRSCR(1)+100000),5):LOCATE 19,0,0:PRINT LVN
9535 LOCATE 25,0,0:COLOR 7:PRINT RIGHT$(STR$(PLRSCR(2)+100000),5)
9540 LINE(0,8)-(319,8),7
9545 RETURN

